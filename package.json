{
  "name": "snapbuild",
  "version": "0.0.117",
  "description": "Downloads binary for your architecture of tool that makes code snapshotable.",
  "main": "snapbuild.js",
  "types": "snapbuild.d.ts",
  "scripts": {
    "prepublish": "yarn build",
    "build": "tsc -p ./tsconfig.json",
    "lint": "prettier -c ./src/",
    "postinstall": "node ./dist/post-install.js"
  },
  "directories": {
    "dist": "./dist"
  },
  "files": [
    "dist/*",
    "snapbuild.d.ts"
  ],
  "dependencies": {
    "debug": "^4.3.1",
    "pkg-up": "^3.1.0",
    "resolve-from": "^5.0.0",
    "snapbuild-darwin-64": "0.0.116"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/node": "^14.14.20",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "repository": "https://github.com/thlorenz/snapbuild.git",
  "author": "Thorsten Lorenz <thlorenz@gmx.de>",
  "license": "MIT",
  "private": false
}
